package 课堂练习;

public class 九九乘法表 {
	public static void main(String[] args) {
		        int size = 9; // 控制乘法表的大小，这里是9x9的乘法表

		        for (int i = 1; i <= size; i++) {
		            for (int j = 1; j <= i; j++) {
		                System.out.print(j + " × " + i + " = " + (i * j) + "\t");
		            }
		            System.out.println();
		        }

	}
}
