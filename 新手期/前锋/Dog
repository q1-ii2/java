package com.qf.cls;

public class Dog {
//		属性：颜色
//		实例变量(成员变量)定义在类的内部，方法的外部
//		数据类型 属性名;
		String color;
//		大小
		int size;
//		声音
		String voice;
//		性别
		String sex;		
//		方法：吃饭
//		public 返回值类型 方法名(形参){}
//		实例方法
		public void eat(){
			System.out.println("狗狗爱吃骨头");
		}
//		睡觉
		public void sleep(){
			System.out.println("狗狗爱睡觉");
		}
//		上厕所
		public void toilet(){
			System.out.println("狗狗爱上厕所");
		}
//		玩
		public void play(){
			System.out.println("狗狗喜欢玩");
			eat();
		}

}
