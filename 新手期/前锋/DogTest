package com.qf.cls;

public class DogTest {
	public static void main(String[] args) {
//		创建对象
//		类名 对象名=new 类名();
//		创建对象时,一定要注意数据的初始化
		Dog dog=new Dog();
//		调用属性:对象名.属性名=值;
		dog.color="白色";
		dog.size=10;
		dog.sex="公";
		dog.voice="汪汪";
//		如果没有给他进行赋值,默认为数据类型的默认值
		System.out.println(dog.color+"\t"+dog.size+"\t"+dog.sex+"\t"+dog.voice+"\t");
		
//			
		dog.play();
//		dog.eat();
	}
	public void a(){
		Dog dog = new Dog();
		dog.play();
	}
}
